{
  "gitbook": "3.x",
  "title": "Enzyme",
  "description": "React Testing",
  "root": "docs/",
  "structure": {
    "summary": "README.md",
    "glossary": "GLOSSARY.md"
  },
  "plugins": [
    "edit-link",
    "prism@2.0.0",
    "-highlight",
    "github",
    "-search",
    "codeblock-disable-glossary",
    "collapsible-menu",
    "anchors"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/airbnb/enzyme/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/airbnb/enzyme/"
    }
  }
}
